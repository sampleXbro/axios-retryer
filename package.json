{
  "name": "axios-retryer",
  "version": "1.4.1",
  "description": "axios-retryer is an advanced Axios request manager offering intelligent retry logic with token refresh, concurrency control, priority queuing, and a flexible plugin architecture, all built with TypeScript for robust HTTP client integrations.",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sampleXbro/axios-retryer.git"
  },
  "scripts": {
    "build": "mkdir -p stats && rollup -c --bundleConfigAsCjs",
    "analyze": "open stats/bundle-stats-*.html",
    "prepare": "npm run build",
    "test": "jest --coverage --silent"
  },
  "keywords": [
    "axios",
    "retry",
    "advanced-retry",
    "priority-queue",
    "typescript",
    "token-refresh",
    "concurrency",
    "queue",
    "plugin",
    "http-client"
  ],
  "author": "sampleX (Serhii Zhabskyi)",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/jest": "^29.5.14",
    "@types/node": "22.13.9",
    "@types/react": "^18.2.0",
    "axios": "^1.8.3",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "prettier": "3.4.2",
    "react": "^18.2.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.34.1",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.2.5",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "axios": ">=1.0.0",
    "react": ">=16.8.0"
  },
  "files": [
    "dist"
  ],
  "directories": {
    "lib": "dist"
  },
  "browser": "dist/browser/axios-retryer.min.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/index.esm.js"
    },
    "./react": {
      "types": "./dist/types/react/index.d.ts",
      "import": "./dist/react/index.esm.js",
      "require": "./dist/react/index.cjs.js",
      "default": "./dist/react/index.esm.js"
    },
    "./react/hooks/useAxiosRetryer": {
      "types": "./dist/types/react/useAxiosRetryer.d.ts",
      "import": "./dist/react/hooks/useAxiosRetryer.esm.js",
      "require": "./dist/react/hooks/useAxiosRetryer.cjs.js"
    },
    "./react/hooks/useAxiosRetryerQuery": {
      "types": "./dist/types/react/useAxiosRetryerQuery.d.ts",
      "import": "./dist/react/hooks/useAxiosRetryerQuery.esm.js",
      "require": "./dist/react/hooks/useAxiosRetryerQuery.cjs.js"
    },
    "./react/hooks/useAxiosRetryerMutation": {
      "types": "./dist/types/react/useAxiosRetryerMutation.d.ts",
      "import": "./dist/react/hooks/useAxiosRetryerMutation.esm.js",
      "require": "./dist/react/hooks/useAxiosRetryerMutation.cjs.js"
    },
    "./react/hooks/useGet": {
      "types": "./dist/types/react/convenience/useGet.d.ts",
      "import": "./dist/react/hooks/useGet.esm.js",
      "require": "./dist/react/hooks/useGet.cjs.js"
    },
    "./react/hooks/usePost": {
      "types": "./dist/types/react/convenience/usePost.d.ts",
      "import": "./dist/react/hooks/usePost.esm.js",
      "require": "./dist/react/hooks/usePost.cjs.js"
    },
    "./react/hooks/usePut": {
      "types": "./dist/types/react/convenience/usePut.d.ts",
      "import": "./dist/react/hooks/usePut.esm.js",
      "require": "./dist/react/hooks/usePut.cjs.js"
    },
    "./react/hooks/useDelete": {
      "types": "./dist/types/react/convenience/useDelete.d.ts",
      "import": "./dist/react/hooks/useDelete.esm.js",
      "require": "./dist/react/hooks/useDelete.cjs.js"
    },
    "./plugins/CachingPlugin": {
      "types": "./dist/types/plugins/CachingPlugin/index.d.ts",
      "import": "./dist/plugins/CachingPlugin.esm.js",
      "require": "./dist/plugins/CachingPlugin.cjs.js"
    },
    "./plugins/CircuitBreakerPlugin": {
      "types": "./dist/types/plugins/CircuitBreakerPlugin/index.d.ts",
      "import": "./dist/plugins/CircuitBreakerPlugin.esm.js",
      "require": "./dist/plugins/CircuitBreakerPlugin.cjs.js"
    },
    "./plugins/TokenRefreshPlugin": {
      "types": "./dist/types/plugins/TokenRefreshPlugin/index.d.ts",
      "import": "./dist/plugins/TokenRefreshPlugin.esm.js",
      "require": "./dist/plugins/TokenRefreshPlugin.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  }
}
